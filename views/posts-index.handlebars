<div class="container">
		<div class="row">
        {{#posts}}
			<div class="[ col-xs-12 col-sm-offset-2 col-sm-8 ]">
				<ul class="event-list">
					<li>

					<li>
						<time datetime="2017-11-24">
							<span class="day">{{this.day}}</span>
							<span class="month">{{this.month}}</span>
							<span class="time">{{this.time}}</span>
						</time>
						<div class="info">

							<div class="row">
								<div class="col-sm-10">
									<h2 class="title">{{this.title}}</h2>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-3">Start:</div>
								<div class="col-sm-9">{{this.month}},{{this.day}} {{this.time}}</div>
							</div>

							<div class="row">
								<div class="col-sm-3">Address:</div>
								<div class="col-sm-9">{{this.address}}</div>
							</div>
              {{/posts}}
						</div>
					</li>





<div class="row">
  {{#posts}}
  <div class="col-sm-8 col-sm-offset-2">
    <li class="list-group-item">
      <a href="/posts/{{this._id}}" class="lead">{{this.title}}</a>
    </br>
      <p>Submitted by {{this.username}}<p>
      <a href="{{this.url}}" target="_blank">{{this.url}}<a>
        <div class="text-right">
        <a href="/n/{{this.category}}">{{this.category}}<a>
      </div>
      <form class="vote-up" data-id="{{post._id}}">
        <button type="submit">Vote Up</button>
      </form>

      <form class="vote-down" data-id="{{post._id}}">
        <button type="submit">Vote Down</button>
      </form>
    </li>
  </div>

    {{/posts}}
</div>
